{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2017",
    "experimental": {
      "keepImportAssertions": false
    }
  },
  "module": {
    "type": "commonjs",
    "strict": true
  },
  "minify": true,
  "sourceMaps": false
}
